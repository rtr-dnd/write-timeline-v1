{
  "buildCommand": "expo export -p web",
  "outputDirectory": "dist/client",
  "functions": {
    "api/index.ts": {
      "runtime": "@vercel/node@5.1.8",
      "includeFiles": "dist/server/**"
    },
    "api/stream-test.ts": {
      "runtime": "@vercel/node@5.1.8"
    }
  },
  "rewrites": [
    {
      "source": "/api/chat",
      "destination": "/api/chat"
    },
    {
      "source": "/api/stream-test",
      "destination": "/api/stream-test"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ]
}
